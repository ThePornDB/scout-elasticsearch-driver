{
    "name": "theporndb/scout-elasticsearch-driver",
    "type": "library",
    "description": "The Elasticsearch Driver for Laravel Scout",
    "keywords": [
        "elastic",
        "elasticsearch",
        "driver",
        "engine",
        "laravel",
        "scout",
        "search"
    ],
    "homepage": "https://babenkoivan.github.io/scout-elasticsearch-driver/",
    "license": "MIT",
    "authors": [
        {
            "name": "Babenko Ivan",
            "email": "babenko.i.a@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Aaron Florey",
            "email": "aaron@buckhamduffy.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-json": "*",
        "elasticsearch/elasticsearch": "^8.0",
        "illuminate/contracts": "^11.0",
        "laravel/scout": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^v1.16",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "ScoutElastic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScoutElastic\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScoutElastic\\ScoutElasticServiceProvider"
            ]
        }
    }
}
